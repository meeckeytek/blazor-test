{
  "navigationFallback": {
    "rewrite": "index.html"
  },
  "globalHeaders": {
    "Content-Encoding": [
      {
        "fileMatch": ".br",
        "encoding": "br"
      },
      {
        "fileMatch": ".gz",
        "encoding": "gzip"
      }
    ],
    "Vary": [
      {
        "fileMatch": ".*",
        "value": "Accept-Encoding"
      }
    ]
  }
}
